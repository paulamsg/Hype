generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id          Int       @id @default(autoincrement())
  name        String
  lastName    String
  username    String    @unique
  email       String    @unique
  password    String    
  location    String
  bio         String?
  avatarUrl   String?
  createdAt   DateTime  @default(now())
}

